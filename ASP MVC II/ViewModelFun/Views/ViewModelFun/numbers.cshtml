<html>
    <head>
       <title>Numbers</title>
       <meta charset='utf-8'/>
        <link rel='stylesheet' type='text/css' href='~/css/styles.css'>
    </head>
    <body>
        @model int[]
        <h1>Here are some numbers!</h1>

        <div style="padding: 10px; border: 1px solid black; width: 150px;">
        @{
            for (var i = 0; i < @Model.Length; i++)
            {
                <p>@Model[i]</p>
            }
        }
        </div>
    </body>
</html>